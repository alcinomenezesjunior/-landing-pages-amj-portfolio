        "operation": "update",
        "table": "appointments",
        "filterType": "string",
        "filterString": "date=eq.={{$json.bookingDate}}",
        "columns": {
          "mappingMode": "defineBelow",
          "value": {
            "deposit_paid": true,
            "status": "confirmed",
            "stripe_payment_id": "={{$json.paymentIntentId}}"
          }
        }
      },
