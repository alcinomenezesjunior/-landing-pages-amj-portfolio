    {
      "parameters": {
        "jsCode": "// Verify Stripe signature (production use)\nconst body = $input.item.json.body;\nconst headers = $input.item.json.headers;\n\n// For demo purposes, we'll skip signature verification\n// In production: verify using stripe signature header + webhook secret\n\nconst event = body;\n\nreturn {\n  json: {\n    eventType: event.type,\n    eventId: event.id,\n    sessionData: event.data?.object || {},\n    rawEvent: event\n  }\n};"
      },
      "id": "code-parse-stripe-event",
      "name": "Code ‚Äì Parse Stripe Event",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        460,
        300
      ]
    },
    {
      "parameters": {
        "conditions": {
          "string": [
            {
              "value1": "={{$json.eventType}}",
              "operation": "equals",
              "value2": "checkout.session.completed"
            }
          ]
        }
      },
      "id": "if-checkout-completed",
      "name": "IF ‚Äì Checkout Completed?",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [
        680,
        300
      ]
    },
    {
      "parameters": {
        "jsCode": "// Extract metadata from Stripe session\nconst session = $input.item.json.sessionData;\n\nconst metadata = session.metadata || {};\n\nreturn {\n  json: {\n    clientPhone: metadata.phone,\n    serviceName: metadata.service,\n    bookingDate: metadata.date,\n    bookingTime: metadata.time,\n    paymentIntentId: session.payment_intent,\n    amountPaid: session.amount_total / 100, // Convert from cents\n    customerEmail: session.customer_email || session.customer_details?.email,\n    sessionId: session.id\n  }\n};"
      },
      "id": "code-extract-metadata",
      "name": "Code ‚Äì Extract Metadata",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        900,
        200
      ]
    },
    {
