    {
      "parameters": {
        "jsCode": "const clientResults = $node['Get Client'].json; \n\nlet output = {\n  clientExists: false,\n  clientData: null \n};\n\n// Se o array tiver itens (o cliente existe), atualiza o objeto de saída\nif (clientResults.length > 0) {\n  output.clientExists = true;\n  output.clientData = clientResults[0]; // Guarda o primeiro cliente encontrado\n}\n\n// Retorna o novo objeto para o nó seguinte (Client Exists?)\nreturn [{ json: output }];"
      },
